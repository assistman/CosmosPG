    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-build-tool" value="gradle" />
    <plugin name="phonegap-plugin-push" spec="2.2.3">
        <param name="SENDER_ID" value="21155591901" />
    </plugin>
    <preference name="phonegap-version"                  value="cli-9.0.0" />
    <preference name="orientation"                       value="default" />
    <preference name="target-device"                     value="universal" />
    <preference name="fullscreen"                        value="true" />
    <preference name="webviewbounce"                     value="false" />
    <preference name="prerendered-icon"                  value="false" />
    <preference name="detect-data-types"                 value="false" />
    <preference name="exit-on-suspend"                   value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false"/>
    <preference name="GapBetweenPages"                   value="0"/>
    <preference name="DisallowOverscroll"                value="true" />
    <preference name="SuppressesIncrementalRendering"    value="false"/>
    <plugin name="cordova-plugin-facebook4" spec="5.0.0">
        <param name="APP_ID" value="2381083715437078" />
        <param name="APP_NAME" value="MARKETS.COM: Indices FX Stocks" />
    </plugin>
    
    <plugin name="cordova-plugin-googleplus" spec="7.0.2">
        <variable name="REVERSED_CLIENT_ID"        value="com.googleusercontent.apps.21155591901-q9e2cs05k7td0i3veg110r2lbdmut1k5" />
        <variable name="WEB_APPLICATION_CLIENT_ID" value="AIzaSyCqRl_-q57A6LRNibxLeg-mH6mZWbtY4oA" />
    </plugin>
    <platform name="ios">
        <resource-file src="GoogleService-Info.plist" />
        <pods-config ios-min-version="9.0" use-frameworks="true">
        </pods-config>
        <pod name="GoogleSignIn" />
        <pod name="SVProgressHUD" />
        <pod name="FBSDKCoreKit" />
        <pod name="FBSDKLoginKit" />
        <pod id="Lock" fix-bundle-path="Lock/Auth0.bundle"/>
        

        

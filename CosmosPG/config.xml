<!--        Network access settings-->
        <access origin='*' allows-arbitrary-loads-for-media='true' allows-arbitrary-loads-in-web-content='true' allows-local-networking='true' />
        <!-- Contact Us QA -->
        <allow-intent href="https://beta-global-qa.markets.com/contact-phone/*" />
        <!-- Contact Us Prod -->
        <allow-intent href="https://www.markets.com/contact-phone/*" />
        <!-- MetaTrader Trading Conditions QA -->
        <allow-intent href="https://beta-global-qa.markets.com/metatrader-trading-conditions/*" />
        <!-- MetaTrader Trading Conditions Prod -->
        <allow-intent href="https://www.markets.com/metatrader-trading-conditions/*" />
        <!-- Account Statement  QA -->
        <allow-intent href="https://cosmos-qa-live.markets.com/dxdeal-report/*" />
        <!-- Account Statement  Prod -->
        <allow-intent href="https://live-trader.markets.com/dxdeal-report/*" />
        <!-- Legal documents  Prod -->
        <allow-intent href="https://beta-global-qa.markets.com/regulation-and-legal-pack/*" />
        <!-- Legal documents  Prod -->
        <allow-intent href="https://www.markets.com/regulation-and-legal-pack/*" />
        <!-- Contact phone  QA -->
        <allow-intent href="https://beta-global-qa.markets.com/contact-phone/*" />
        <!-- Contact phone Prod -->
        <allow-intent href="https://www.markets.com/contact-phone/*" />
        <allow-intent href="telprompt:*" />
        <allow-intent href="tel:*" />
        <allow-intent href="sms:*" />
        <allow-intent href="mailto:*" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-intent href="file:*" />
        <allow-navigation href="*" />
        <config-file target="*-Info.plist" parent="UIBackgroundModes">
            <array>
                <string>fetch</string>
                <string>remote-notification</string>
            </array>
        </config-file>
        
        <config-file target="*-Info.plist" parent="NSPhotoLibraryUsageDescription">
            <string>It will give you an option to browse the document photos on the device.</string>
        </config-file>
        
        <config-file target="*-Info.plist" parent="NSCameraUsageDescription">
            <string>It will give possibility to make a photo of the document</string>
        </config-file>
        
        <config-file target="*-Info.plist" parent="CFBundleShortVersionString">
            <string>1.76.0</string>
        </config-file>
